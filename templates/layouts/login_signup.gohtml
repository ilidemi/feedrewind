  <link rel="icon" sizes="96x96" href="{{static "favicon_96x96.png"}}">
  <link rel="icon" sizes="192x192" href="{{static "favicon_192x192.png"}}">
  <link rel="apple-touch-icon" href="{{static "apple-touch-icon.png"}}">
  <link rel="stylesheet" media="screen" href="{{static "tailwind.css"}}">
  <meta name="csrf-param" content="authenticity_token">
  <meta name="csrf-token" content="{{.Session.CSRFToken}}">
